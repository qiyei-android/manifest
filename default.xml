<?xml version="1.0" encoding="UTF-8"?>
<manifest>
     <remote name="android" fetch="https://github.com/qiyei-android/"/>

     <default revision="main" remote="android" sync-j="4" />

	 <!--name 子仓库的 git 仓库名称,path repo sync 同步时，相对于根目录的子仓库文件夹路径 分组 revision: 使用的分支名 remote：关联的remote名-->
	 
	 <project name="common" path="library/common" groups="null" revision="main" remote="android"/>
     <project name="http" path="library/http" groups="null" revision="main" remote="android"/>
     <project name="imageloader" path="library/imageloader" groups="null" revision="main" remote="android"/>
	 <project name="cachemanager" path="library/cachemanager" groups="null" revision="main" remote="android"/>
	 
	 <project name="provider" path="framework/provider" groups="null" revision="main" remote="android"/>
</manifest>